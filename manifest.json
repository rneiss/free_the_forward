{
  "name": "FreeTheForward",
  "version": "0.1",
  "manifest_version": 2,
  "description": "An extension that bypasses the Forward's paywall by pretending to be google ad bot",
  "background": {
    "scripts": ["free_the_forward.js"],
    "persistent": true
  },
  "content_scripts": [{
    "matches": ["*://*.forward.com/*"],
    "css": ["free_the_forward.css"]
  }],
  "permissions": [
    "contentSettings",
    "webRequest",
    "webRequestBlocking",
    "http://*.forward.com/"
  ]
}
